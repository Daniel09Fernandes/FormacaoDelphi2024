unit Form.Visualiza.Hash;

interface

uses
  Winapi.Windows, Winapi.Messages, System.SysUtils, System.Variants, System.Classes, Vcl.Graphics,
  Vcl.Controls, Vcl.Forms, Vcl.Dialogs, Vcl.StdCtrls;

type
  TFormVisualizaHasg = class(TForm)
    StaticText1: TStaticText;
    Button1: TButton;
    procedure Button1Click(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
    procedure SetHash(pHash: string);
  end;

var
  FormVisualizaHasg: TFormVisualizaHasg;

implementation

{$R *.dfm}

procedure TFormVisualizaHasg.Button1Click(Sender: TObject);
begin
  Close;
end;

procedure TFormVisualizaHasg.SetHash(pHash: string);
begin
  StaticText1.Caption := pHash;
end;

end.
